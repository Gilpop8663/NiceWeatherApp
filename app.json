{
  "expo": {
    "name": "NiceWeatherApp",
    "slug": "NiceWeatherApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "description": "First React Naitve App  // expo use",
    "githubUrl": "https://github.com/Gilpop8663/NiceWeatherApp"
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "description": "First React Naitve App  // expo use",
  "githubUrl": "https://github.com/Gilpop8663/NiceWeatherApp"
}
